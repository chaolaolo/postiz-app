{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true,
      "dynamicImport": true
    },
    "target": "es2020",
    "baseUrl": "/Users/nevodavid/Projects/gitroom",
    "paths": {
      "@chaolaolo/backend/*": [
        "apps/backend/src/*"
      ],
      "@chaolaolo/cron/*": [
        "apps/cron/src/*"
      ],
      "@chaolaolo/frontend/*": [
        "apps/frontend/src/*"
      ],
      "@chaolaolo/helpers/*": [
        "libraries/helpers/src/*"
      ],
      "@chaolaolo/nestjs-libraries/*": [
        "libraries/nestjs-libraries/src/*"
      ],
      "@chaolaolo/react/*": [
        "libraries/react-shared-libraries/src/*"
      ],
      "@chaolaolo/plugins/*": [
        "libraries/plugins/src/*"
      ],
      "@chaolaolo/workers/*": [
        "apps/workers/src/*"
      ],
      "@chaolaolo/extension/*": [
        "apps/extension/src/*"
      ]
    },
    "keepClassNames": true,
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "loose": true
  },
  "module": {
    "type": "commonjs",
    "strict": false,
    "strictMode": true,
    "lazy": false,
    "noInterop": false
  },
  "sourceMaps": true,
  "minify": false
}